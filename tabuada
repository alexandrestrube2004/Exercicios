 import java.util.Scanner;
public class tabuada{
    public static void main (String [] args){
        Scanner ler= new Scanner (System.in);
        int n;
        do{
        System.out.printf ("\n Digite um valro de 1 a 9:");
        n= ler.nextInt();
       }while (n>9 || n<1);
       
    }
}      
