import java.util.Scanner;
public class multiplos_de_quatro{
    public static void main (String [] args){
        Scanner ler= new Scanner (System.in);
        int v1, v2;
        
        System.out.printf ("Digite o maior valor:");
        v1= ler.nextInt();
        
        System.out.printf ("Digite o menor valor:");
        v2= ler.nextInt();
        
        for (int i=v1; i>=v2;i--){
        if (i%4==0)System.out.println (i);
        }
    
   }
}
